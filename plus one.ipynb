   "source": [
    "\n",
    "digits = [4,3,2,1]\n",
    "class Solution:\n",
    "    def plusOne(self, digits):\n",
    "        \n",
    "        o=\"\"\n",
    "        for i in digits:\n",
    "            o=o+str(i)\n",
    "        output=int(o)+1\n",
    "        l=list(\"\".join(str(output)))\n",
    "        return l\n",
    "akar=Solution()\n",
    "akar.plusOne(digits)"
   ]
